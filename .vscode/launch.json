{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "coreclr",
      "request": "launch",
      "name": ".NET Launch (HTTP)",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/drawiomvc/bin/Debug/net9.0/drawiomvc.dll",
      "cwd": "${workspaceFolder}/drawiomvc",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "http://localhost:5176"
      },
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    },
    {
      "type": "coreclr",
      "request": "launch",
      "name": ".NET Launch (HTTPS)",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/drawiomvc/bin/Debug/net9.0/drawiomvc.dll",
      "cwd": "${workspaceFolder}/drawiomvc",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ASPNETCORE_URLS": "https://localhost:7270;http://localhost:5176"
      },
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
      }
    }
  ]
}
