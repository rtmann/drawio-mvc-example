{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build",
      "type": "process",
      "command": "dotnet",
      "args": [
        "build",
        "${workspaceFolder}/drawiomvc/drawiomvc.csproj",
        "/property:GenerateFullPaths=true",
        "/consoleloggerparameters:NoSummary"
      ],
      "group": "build",
      "problemMatcher": ["$msCompile"]
    },
    {
      "label": "docker: build drawiomvc",
      "type": "shell",
      "command": "docker",
      "args": [
        "build",
        "-t",
        "drawiomvc",
        "."
      ],
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "problemMatcher": []
    }
  ]
}
